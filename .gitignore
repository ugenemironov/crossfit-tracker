cat > .gitignore << 'EOF'
# Node modules
node_modules/
*/node_modules/

# Environment variables
.env
*/.env

# Database files
*.db
*.sqlite
*.sqlite3

# Logs
*.log
npm-debug.log*

# OS files
.DS_Store
Thumbs.db

# IDE
.vscode/
.idea/

# Build files
frontend/build/
frontend/.eslintcache

# Temporary files
*.tmp
*.swp
EOF